{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Environment": "Development",
  "Cors": {
    "Origins": [
      "https://localhost:3000"
    ]
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=fastapi;Username=postgres;Password="
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {CorrelationId} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Jwt": {
    "Secret": "WhrSelWy0arNLNakzrc5FeQmsNPvoBk2qBZCexsnNDI=",
    "Issuer": "FastNet",
    "Audience": "FastNet",
    "ExpiryMinutes": 3600
  }
}