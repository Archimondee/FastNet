{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Environment": "Development",
  "Cors": {
    "Origins": [
      "https://localhost:3000"
    ]
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=fastapi;Username=postgres;Password=",
    "QuartzDb": "Host=localhost;Port=5432;Database=quartzdb;Username=postgres;Password="
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {CorrelationId} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Jwt": {
    "Secret": "WhrSelWy0arNLNakzrc5FeQmsNPvoBk2qBZCexsnNDI=",
    "Issuer": "FastNet",
    "Audience": "FastNet",
    "ExpiryMinutes": 3600
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "FastNetScheduler",
    "quartz.scheduler.instanceId": "AUTO",
    "quartz.threadPool.type": "Quartz.Simpl.SimpleThreadPool, Quartz",
    "quartz.threadPool.threadCount": 5,
    "quartz.jobStore.type": "Quartz.Impl.AdoJobStore.JobStoreTX, Quartz",
    "quartz.jobStore.useProperties": "true",
    "quartz.jobStore.dataSource": "Default",
    "quartz.jobStore.tablePrefix": "QRTZ_",
    "quartz.jobStore.driverDelegateType": "Quartz.Impl.AdoJobStore.PostgreSQLDelegate, Quartz",
    "quartz.dataSource.Default.provider": "Npgsql",
    "quartz.dataSource.Default.connectionString": "Host=localhost;Port=5432;Database=quartzdb;Username=postgres;Password=yourpassword",
    "quartz.dataSource.Default.maxConnectionPoolSize": 10
  }
}